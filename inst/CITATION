citHeader("To cite predictiveMargins in publications use:")

citEntry(entry = "Manual",
  title        = "{predictiveMargins}: Predictive margins for random forests",
  author       = personList(as.person("Jason Grafmiller"),
                   as.person("Lukas Sönning")),
  year         = "2022",
  note         = "R package version 0.10.1",
  url          = "https://github.com/jasongraf1/predictiveMargins",

  textVersion  =
  paste("Grafmiller, Jason and Lukas Sönning. (2022).",
        "{predictiveMargins}: Predictive margins for random forests.",
        "R package version 0.10.1, ",
        "URL https://github.com/jasongraf1/predictiveMargins.")
)
